{
  "name": "shinkansen-pantograph",
  "version": "1.0.19",
  "description": "Shinkansen Pantograph",
  "keywords": [
    "Shinkansen",
    "Pantograph",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "Router"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.18.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-pantograph.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=test mocha -r @babel/register test --recursive",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib"
  },
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^8.1.3",
    "rimraf": "^3.0.2",
    "shinkansen-rails": "1.0.15",
    "shinkansen-signals": "1.0.15",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0"
  },
  "peerDependencies": {
    "shinkansen-rails": "1.0.15",
    "shinkansen-signals": "1.0.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
