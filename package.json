{
  "name": "shinkansen-pantograph",
  "version": "1.0.3",
  "description": "Shinkansen Pantograph",
  "main": "./index.js",
  "keywords": [
    "Shinkansen",
    "Pantograph",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "Router"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/modernpoacher/shinkansen-pantograph.git"
  },
  "author": "Jonathan Perry for Modern Poacher Limited",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run lint:fix && npm run babel",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src test *.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha -r @babel/register test/**/*.spec.js",
    "pre-commit": "npm run lint && npm test && npm run babel && git add lib"
  },
  "dependencies": {
    "shinkansen-rails": "1.0.3",
    "shinkansen-signals": "1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "rimraf": "^3.0.0",
    "sinon": "^8.0.1",
    "sinon-chai": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
