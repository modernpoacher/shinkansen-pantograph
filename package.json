    "#pantograph/pantograph": "./src/pantograph/index.mjs",
    "#pantograph/pantograph/*": "./src/pantograph/*.mjs"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./pantograph": "./src/pantograph/index.mjs",
    "./pantograph/*": "./src/pantograph/*.mjs"
  }
}
