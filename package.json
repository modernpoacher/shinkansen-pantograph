{
  "name": "shinkansen-pantograph",
  "version": "1.5.28",
  "description": "Shinkansen Pantograph",
  "keywords": [
    "Shinkansen",
    "Pantograph",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "Router"
  ],
  "main": "./src/index.mjs",
  "type": "module",
  "types": "./index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/modernpoacher/shinkansen-pantograph.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "nvm": "bash nvm.sh",
    "prelint": "npm run nvm",
    "prepare": "husky",
    "pretest": "npm run nvm",
    "test": "cross-env NODE_ENV=test mocha test --recursive"
  },
  "dependencies": {
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@sequencemedia/eslint-config-standard": "^0.2.48",
    "@sequencemedia/eslint-config-typescript": "^0.1.74",
    "@sequencemedia/sinon-chai": "^0.0.140",
    "@types/babel__register": "^7.17.3",
    "@types/chai": "^5.2.1",
    "@types/debug": "^4.1.12",
    "@types/mocha": "^10.0.10",
    "@types/sinon": "^17.0.4",
    "chai": "^5.2.0",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "mocha": "^11.1.0",
    "shinkansen-rails": "1.1.19",
    "shinkansen-signals": "1.2.18",
    "sinon": "^20.0.0"
  },
  "imports": {
    "#pantograph": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
