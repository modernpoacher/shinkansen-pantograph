{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ESNext",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#pantograph": [
        "src/index.cjs",
        "src/index.mjs"
      ],
      "#pantograph/pantograph": [
        "src/pantograph/index.mjs"
      ],
      "#pantograph/pantograph/pantograph": [
        "src/pantograph/pantograph.mjs"
      ]
    }
  },
  "include": [
    "."
  ],
  "exclude": [
    "test/**/*"
  ]
}
